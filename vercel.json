{
    "cleanUrls": true,
    "headers": [
      {
        "source": "/api/_content/query/(.+)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ]
      }
    ],
    "redirects": [
        { "source": "/about/photos*", "destination": "/photos*" },
        { "source": "/about/videos*", "destination": "/videos*" },
        { "source": "/about/library*", "destination": "/library*" },
        { "source": "/about/gear", "destination": "/about" },
        { "source": "/about/credits", "destination": "/credits" },
        { "source": "/work/services", "destination": "/services" },
        { "source": "/work/experience", "destination": "/resume" },
        { "source": "/work/resume", "destination": "/resume" },
        { "source": "/tutorials/upcoming", "destination": "/tutorials" },
        { "source": "/tutorials/sponsor", "destination": "/tutorials" },
        { "source": "/tutorials/topic/*", "destination": "/tutorials/tag*" }
    ],
    "trailingSlash": false
}